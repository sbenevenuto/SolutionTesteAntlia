@model Model.Product.ProductCosif

@{
    ViewData["Title"] = "Create";
}

<h4>Novo @Resource.Resources.LBL_PRODUCT_COSIF</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="CodProductCosif" class="control-label"></label>
                <input asp-for="CodProductCosif" class="form-control" />
                <span asp-validation-for="CodProductCosif" class="text-danger"></span>
            </div>           
            <div class="form-group">
                <input type="submit" value="Salvar" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
